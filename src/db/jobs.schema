CREATE TABLE jobs (
  id INTEGER PRIMARY KEY,
  job_id INT UNIQUE,
  job_name TEXT NOT NULL,
  status INT CHECK( status IN (1,2,3,4,5,6,7,8,9) ) NOT NULL,
  finished_at TEXT,
  ref TEXT,
  author TEXT NOT NULL,
  autor_avatar_url TEXT NOT NULL,
  pipeline_id INT NOT NULL,
  pipeline_url TEXT NOT NULL,
  duration REAL,
  queue_duration REAL,
  coverage TEXT
);